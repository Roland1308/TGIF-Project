A;AACA,OAPA,MAOA;AACA,aAAA,aAAA,CAAA,OAAA;AACA,aAAA,QAAA,CAAA,MAAA,EAAA,IAAA,IAAA,CAAA,CAAA;AACA,aAAA,QAAA,CAAA,MAAA,EAAA,KAAA,GAAA,KAAA,QAAA,CAAA,MAAA,EAAA,IAAA,IAAA,CAAA,CAAA,GACA,KAAA,QAAA,CAAA,MAAA,EAAA,IADA,GAEA,KAAA,QAAA,CAAA,MAAA,EAAA,EAFA;AAGA;;AACA,WAAA,eAAA,GAAA,KAAA,aAAA;AACA,KAlBA;AAmBA,IAAA,WAAA,EAAA,qBAAA,CAAA,EAAA;AACA,eAAA,OAAA,CAAA,CAAA,EAAA,CAAA,EAAA;AACA,gBAAA,CAAA;AACA,eAAA,CAAA;AACA,gBAAA,CAAA,CAAA,SAAA,GAAA,CAAA,CAAA,SAAA,EAAA,OAAA,CAAA,CAAA,CAAA,KACA,OAAA,CAAA;;AACA,eAAA,CAAA;AACA,gBAAA,CAAA,CAAA,UAAA,GAAA,CAAA,CAAA,UAAA,EAAA,OAAA,CAAA,CAAA,CAAA,KACA,OAAA,CAAA;;AACA,eAAA,CAAA;AACA,gBAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA,EAAA,OAAA,CAAA,CAAA,CAAA,KACA,OAAA,CAAA;;AACA,eAAA,CAAA;AACA,gBAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA,EAAA,OAAA,CAAA,CAAA,CAAA,KACA,OAAA,CAAA;;AACA,eAAA,CAAA;AACA,mBAAA,CAAA,CAAA,SAAA,GAAA,CAAA,CAAA,SAAA;;AACA,eAAA,CAAA;AACA,mBAAA,CAAA,CAAA,oBAAA,GAAA,CAAA,CAAA,oBAAA;AAhBA;AAkBA;;AACA,aAAA,KAAA,aAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,KAzCA;AA2CA,IAAA,UA3CA,sBA2CA,KA3CA,EA2CA;AAAA;;AACA,WAAA,UAAA,GAAA,KAAA;AACA,WAAA,IAAA,GAAA,KAAA,aAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,KAAA,IAAA,KAAA,CAAA,aAAA,IAAA,KAAA,CAAA,aAAA,IAAA,KAAA;AAAA,OADA,CAAA;AAGA,WAAA,IAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,eACA,CAAA,CAAA,KAAA,IAAA,KAAA,CAAA,UAAA,CAAA,CAAA,CAAA,IACA,CAAA,CAAA,KAAA,IAAA,KAAA,CAAA,UAAA,CAAA,CAAA,CADA,IAEA,CAAA,CAAA,KAAA,IAAA,KAAA,CAAA,UAAA,CAAA,CAAA,CAHA;AAAA,OADA,CAAA;AAMA,WAAA,IAAA,CAAA,MAAA,IAAA,CAAA,GACA,KAAA,KAAA,GACA,sEAFA,GAGA,KAAA,KAAA,GACA,+DAJA;AAKA,aAAA,KAAA,IAAA;AACA,KA5DA;AA8DA,IAAA,UA9DA,sBA8DA,KA9DA,EA8DA;AAAA;;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,IAAA,GAAA,KAAA,aAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,KAAA,IAAA,MAAA,CAAA,aAAA,IAAA,MAAA,CAAA,aAAA,IAAA,KAAA;AAAA,OADA,CAAA;AAGA,WAAA,IAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,eACA,CAAA,CAAA,KAAA,IAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAAA,IACA,CAAA,CAAA,KAAA,IAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CADA,IAEA,CAAA,CAAA,KAAA,IAAA,MAAA,CAAA,UAAA,CAAA,CAAA,CAHA;AAAA,OADA,CAAA;AAMA,WAAA,IAAA,CAAA,MAAA,IAAA,CAAA,GACA,KAAA,KAAA,GACA,sEAFA,GAGA,KAAA,KAAA,GACA,+DAJA;AAKA,aAAA,KAAA,IAAA;AACA;AA/EA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"principale\">\r\n    <h2>Senators</h2>\r\n    <p>\r\n      First convened in 1789, the composition and powers of the Senate are\r\n      established in Article One of the U.S. Constitution. Each state is\r\n      represented by two senators, regardless of population, who serve\r\n      staggered six-year terms. The Senate has several exclusive powers not\r\n      granted to the House, including consenting to treaties as a precondition\r\n      to their ratification and consenting to or confirming appointments of\r\n      Cabinet secretaries, federal judges, other federal executive officials,\r\n      military officers, regulatory officials, ambassadors, and other federal\r\n      uniformed officers, as well as trial of federal officials impeached by\r\n      the House.\r\n    </p>\r\n    <div id=\"loadedPage\">\r\n      <div class=\"row align-items-end container\" id=\"filters\">\r\n        <filtering\r\n          :states=\"this.statesSenate\"\r\n          v-on:partyChange=\"funzioneCK\"\r\n          v-on:stateChange=\"funzioneST\"\r\n        ></filtering>\r\n      </div>\r\n      <!--Table-->\r\n      <table class=\"table table-bordered table-hover table-sm\">\r\n        <thead>\r\n          <tr>\r\n            <th>\r\n              Senator:\r\n              <i id=\"name\" :class=\"flagSort[1].arrow\" v-on:click=\"sortColumn(1)\">First Name</i>,\r\n              <i id=\"surname\" :class=\"flagSort[0].arrow\" v-on:click=\"sortColumn(0)\">Last Name</i>\r\n            </th>\r\n            <th>\r\n              <i id=\"party\" :class=\"flagSort[2].arrow\" v-on:click=\"sortColumn(2)\">Party</i>\r\n            </th>\r\n            <th>\r\n              <i id=\"state\" :class=\"flagSort[3].arrow\" v-on:click=\"sortColumn(3)\">State</i>\r\n            </th>\r\n            <th>\r\n              <i id=\"seniority\" :class=\"flagSort[4].arrow\" v-on:click=\"sortColumn(4)\">Seniority</i>\r\n            </th>\r\n            <th>\r\n              <i\r\n                id=\"percentage\"\r\n                :class=\"flagSort[5].arrow\"\r\n                v-on:click=\"sortColumn(5)\"\r\n              >Percentage of votes</i>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody id=\"members_table\">\r\n          <tr v-for=\"(person, idx) in this.list\" :key=\"idx\">\r\n            <td>\r\n              <a v-bind:href=\"person.url\" target=\"_blank\">\r\n                {{person.first_name}} {{person.middle_name}}\r\n                {{person.last_name}}\r\n              </a>\r\n            </td>\r\n            <td>{{person.party}}</td>\r\n            <td>{{person.state}}</td>\r\n            <td>{{person.seniority}}</td>\r\n            <td>{{person.votes_with_party_pct}}%</td>\r\n          </tr>\r\n        </tbody>\r\n        <tfoot id=\"members_foot\" v-html=\"piede\"></tfoot>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport filtering from \"./filtering\";\r\nexport default {\r\n  name: \"senatedata\",\r\n  props: [\r\n    \"membersSenate\",\r\n    \"statesSenate\",\r\n    \"flagSort\"\r\n  ],\r\n  components: {\r\n    filtering\r\n  },\r\n  data() {\r\n    return {\r\n      list: this.membersSenate,\r\n      checkBoxes: [],\r\n      selectedState: \"all\",\r\n      piede: '<tr class=\"normal_foot\"><td colspan=\"5\">END OF LIST</td></tr>',\r\n      oldColumn: 0\r\n    };\r\n  },\r\n  methods: {\r\n    //Sort\r\n    sortColumn(column) {\r\n      if (column != this.oldColumn) {\r\n        this.flagSort[this.oldColumn].flag = 0;\r\n        this.flagSort[this.oldColumn].arrow = this.flagSort[this.oldColumn].no;\r\n        this.flagSort[column].flag = -1;\r\n        this.flagSort[column].arrow = this.flagSort[column].down;\r\n        this.oldColumn = column;\r\n        this.sortedArray(column);\r\n      } else {\r\n        this.membersSenate.reverse();\r\n        this.flagSort[column].flag *= -1;\r\n        this.flagSort[column].arrow = this.flagSort[column].flag == -1 ?\r\n          this.flagSort[column].down :\r\n          this.flagSort[column].up;\r\n      }\r\n      this.filteredMembers = this.membersSenate;\r\n    },\r\n    sortedArray: function (y) {\r\n      function compare(a, b) {\r\n        switch (y) {\r\n          case 0:\r\n            if (a.last_name < b.last_name) return -1;\r\n            else return 1;\r\n          case 1:\r\n            if (a.first_name < b.first_name) return -1;\r\n            else return 1;\r\n          case 2:\r\n            if (a.party < b.party) return -1;\r\n            else return 1;\r\n          case 3:\r\n            if (a.state < b.state) return -1;\r\n            else return 1;\r\n          case 4:\r\n            return a.seniority - b.seniority;\r\n          case 5:\r\n            return a.votes_with_party_pct - b.votes_with_party_pct;\r\n        }\r\n      }\r\n      return this.membersSenate.sort(compare);\r\n    },\r\n\r\n    funzioneCK(check) {\r\n      this.checkBoxes = check;\r\n      this.list = this.membersSenate.filter(\r\n        x => x.state == this.selectedState || this.selectedState == \"all\"\r\n      );\r\n      this.list = this.list.filter(\r\n        x =>\r\n          x.party != this.checkBoxes[0] &&\r\n          x.party != this.checkBoxes[1] &&\r\n          x.party != this.checkBoxes[2]\r\n      );\r\n      this.list.length == 0\r\n        ? (this.piede =\r\n            '<tr class=\"error_foot\"><td colspan=\"5\">NO ITEMS TO DISPLAY</td></tr>')\r\n        : (this.piede =\r\n            '<tr class=\"normal_foot\"><td colspan=\"5\">END OF LIST</td></tr>');\r\n      return this.list;\r\n    },\r\n\r\n    funzioneST(stato) {\r\n      this.selectedState = stato;\r\n      this.list = this.membersSenate.filter(\r\n        x => x.state == this.selectedState || this.selectedState == \"all\"\r\n      );\r\n      this.list = this.list.filter(\r\n        x =>\r\n          x.party != this.checkBoxes[0] &&\r\n          x.party != this.checkBoxes[1] &&\r\n          x.party != this.checkBoxes[2]\r\n      );\r\n      this.list.length == 0\r\n        ? (this.piede =\r\n            '<tr class=\"error_foot\"><td colspan=\"5\">NO ITEMS TO DISPLAY</td></tr>')\r\n        : (this.piede =\r\n            '<tr class=\"normal_foot\"><td colspan=\"5\">END OF LIST</td></tr>');\r\n      return this.list;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n  thead th {\r\n    /* display: table-header-group; */\r\n    position: sticky;\r\n    top: 0px;\r\n    background-color: rgb(221, 221, 221);\r\n    border-bottom: 2px solid #dee2e6;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}